<h1 id="results-message"><%= @store_information[:total] %> Total Stores</h1>
<table class="table stores">
  <thead>
    <tr>
      <th>Store Name</th>
      <th>City</th>
      <th>Distance (mi)</th>
      <th>Phone Number</th>
      <th>Store Type</th>
    </tr>
  </thead>
  <tbody>
    <% @store_information[:stores].each do |store| %>
      <tr>
        <td><%= store.name %></td>
        <td><%= store.city %></td>
        <td><%= store.distance %></td>
        <td><%= store.phone_number %></td>
        <td><%= store.store_type %></td>
      </tr>
    <% end %>
    <tr>
      <td>Mark</td>
      <td>Otto</td>
      <td>@mdo</td>
    </tr>
  </tbody>
</table>
